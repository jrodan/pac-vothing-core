<assembly>
    <id>vothing-zip</id>
    <formats>
        <format>zip</format>
    </formats>
    <includeBaseDirectory>false</includeBaseDirectory>
    <dependencySets>
        <dependencySet>
            <outputDirectory>/Vothing App/deployable</outputDirectory>
            <unpack>false</unpack>
            <scope>runtime</scope>
            <includes>
                <include>*:jar</include>
            </includes>
        </dependencySet>
        <dependencySet>
            <outputDirectory>/Vothing App/javadoc</outputDirectory>
            <unpack>false</unpack>
            <scope>runtime</scope>
            <includes>
                <include>*:javadoc</include>
            </includes>
        </dependencySet>
        <dependencySet>
            <outputDirectory>/Vothing App/sources</outputDirectory>
            <unpack>false</unpack>
            <scope>runtime</scope>
            <includes>
                <include>*:sources</include>
            </includes>
        </dependencySet>
        <dependencySet>
            <outputDirectory>/Vothing App/test-sources</outputDirectory>
            <unpack>false</unpack>
            <scope>runtime</scope>
            <includes>
                <include>*:tests-sources</include>
            </includes>
        </dependencySet>
    </dependencySets>
    <fileSets>
        <!--<fileSet>-->
            <!--<outputDirectory>/Vothing App/documentation</outputDirectory>-->
            <!--<directory>documentation</directory>-->
        <!--</fileSet>-->
        <fileSet>
            <outputDirectory>/Vothing App/sql</outputDirectory>
            <directory>sql</directory>
        </fileSet>
        <fileSet>
            <outputDirectory>/Vothing App/</outputDirectory>
            <directory>..</directory>
            <includes>
                <include>README.md</include>
            </includes>
        </fileSet>
    </fileSets>
</assembly>